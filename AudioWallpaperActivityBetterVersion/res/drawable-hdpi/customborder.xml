<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            
            
            <padding android:left="1dp" android:top="1dp" android:right="1dp"
                android:bottom="1dp" />
            <stroke android:width="13dp" android:height="13dp" android:color="#000000" />
            

            <corners android:radius="1dp" android:bottomRightRadius="5dp"
                android:bottomLeftRadius="5dp" android:topLeftRadius="5dp"
                android:topRightRadius="5dp" />
            
            
            
            
        </shape>
    </item>
    
    <item>
        <shape android:shape="rectangle">
            
            
            <padding android:left="3dp" android:top="3dp"  />
           
            

            <corners android:radius="1dp" android:bottomRightRadius="5dp"
                android:bottomLeftRadius="5dp" android:topLeftRadius="5dp"
                android:topRightRadius="5dp" />
            
            <stroke android:width="10dp" android:height="10dp"
                android:color="#FFFFFF" />
            
            
        </shape>
    </item>

</layer-list>
